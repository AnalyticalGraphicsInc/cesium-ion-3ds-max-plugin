    "version": "0.2.0",
    "configurations": [

        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/C#/bin/Debug/netcoreapp3.0/C#.dll",
            // "args": ["gettoken", "56", "http://localhost:5000/", "<tokenpath>"],
            "args": [ "upload", "<filepath>", "Test", " ", " ", "3DTILES", "3D_MODEL", "AUTO", "<tokenpath>", "<logpath>" ],
            "cwd": "${workspaceFolder}/C#",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}